{
  "comments": {
    "lineComment": "#"
  },
  "autoClosingPairs": [
    [
      "(",
      ")"
    ],
    [
      "[",
      "]"
    ],
    [
      "{",
      "}"
    ]
  ],
  "surroundingPairs": [
    [
      "(",
      ")"
    ],
    [
      "[",
      "]"
    ],
    [
      "{",
      "}"
    ]
  ],
  "wordPattern": "([^#$'-,.:-?A-^{-}\\s]|\\$[1-9A-Z])+|[A-Z]|\\\\[1-9]|#{2,}|[+%]",
  "folding": {
    "offSide": false
  },
  "autoCloseBefore": ";}])> \n\t"
}

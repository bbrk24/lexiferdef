{
  "include": [
    "src/*.ts"
  ],
  "compilerOptions": {
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitReturns": true,
    "strict": true,
    "noEmitOnError": true,
    "outDir": "./dist/",
    "target": "ES2019",
    "lib": [
      "ES2019"
    ],
    "module": "commonjs"
  }
}
